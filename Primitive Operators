import java.util.Scanner;
import java.lang.*;

public class Main {

   public static void main(String[] args) {
       Scanner scanner = new Scanner(System.in);
       //get salaries
       int salary1 = scanner.nextInt();
       int salary2 = scanner.nextInt();
       //your code goes here
       int total = salary1 + salary2;
       System.out.println(total);

   }
}
